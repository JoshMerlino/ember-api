{
	"compilerOptions": {
		"lib": [
			"ES2022"
		],
		"module": "commonjs",
		"target": "ES2022",
		"types": [
			"node"
		],
		"outDir": "lib",
		"jsx": "react",
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"allowUnreachableCode": false,
		"preserveConstEnums": false,
		"resolveJsonModule": true,
		"declaration": true
	},
	"exclude": [
		"node_modules",
		"lib"
	]
}